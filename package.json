{
  "name": "etl-excel",
  "version": "2.0.0",
  "description": "extract, transform and load excel files into target data sources",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "npm run build && nodemon e2e/server.js",
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khattab88/etl-excel.git"
  },
  "author": "Excel Systems",
  "keywords": [
    "etl",
    "excel"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/khattab88/etl-excel/issues"
  },
  "homepage": "https://github.com/khattab88/etl-excel#readme",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.172",
    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "jest": "^27.2.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.0",
    "ts-jest": "^27.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.3"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "lodash": "^4.17.21",
    "printj": "^1.3.0",
    "xlsx": "^0.17.1"
  }
